<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="mainVBox" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="120.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.instrument.newInstrumentProducerViewController">
    <children>
        <Label text="PRODUCENT PRZYRZĄDU">
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </VBox.margin>
        </Label>
        <TextField fx:id="newInstrumentProducerTextField" promptText="wprowadź producenta przyrządu">
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </VBox.margin>
        </TextField>
        <Label fx:id="newInstrumentProducerLabel" textFill="RED">
            <VBox.margin>
                <Insets left="5.0" />
            </VBox.margin></Label>
        <HBox alignment="CENTER" prefHeight="48.0" prefWidth="290.0">
            <children>
                <Button fx:id="addNewInstrumentProducerButton" mnemonicParsing="false" onAction="#addNewProducer" text="Dodaj" textAlignment="CENTER">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </HBox.margin>
                </Button>
                <Button fx:id="cancelNewInstrumentProducerButton" mnemonicParsing="false" onAction="#cancelAddNewProducer" text="Anuluj" textAlignment="CENTER">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                    </HBox.margin>
                </Button>
            </children>
            <VBox.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </VBox.margin>
        </HBox>
    </children>
</VBox>
